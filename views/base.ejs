
    <%- include('header') -%>
    <form  action="/dashboard" class="py-3">
    <button type="submit" class="btn btn-success">Home</button>
</form>


    <div class="text-center center-div" id="login">
        <div class="container w-25 border py-5">
            <div class="title pb-5">
                <h2 class="font-weight-bold">Login</h2>
                <span>Login for the existing user</span>
                <%if(locals.logout){ %>
                    <div class="alter alter-success text-center">
                       <%= logout %>
                    </div>
                <% } %>
            </div>
            <form action="/route/logined" method="post">
            <div class="form-group">
                <input type="email" class="form-control" placeholder="email" name="email">
                <small class="form-text text-muted text-left"> Register email address</small>
            </div>
            <div class="form-group py-3">
                <input type="password" class="form-control" name="password" placeholder="password">
            </div>
            <div class="form-group py-3">
                <button type="submit" class="btn btn-success">Submit</button>
            </div>

            </form>
        </div>
    </div>

    <%- include('footer') -%>
